{
  "name": "bellm/myespacio",
  "description": "An application for showing music and photographs, with advanced search features and enhancement from relevant third party APIs",
  "type": "project",
  "autoload": {
    "psr-4": {
      "MyEspacio\\": "src",
      "Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Mark Bellingham",
      "email": "mail@markbellingham.uk"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1.2",
    "symfony/http-foundation": "^5.3",
    "nikic/fast-route": "^1.3",
    "twig/twig": "^3.3",
    "rdlowrey/auryn": "^1.4",
    "ramsey/uuid": "^4.2",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "phpmailer/phpmailer": "^6.7",
    "messagebird/php-rest-api": "^3.1",
    "ext-exif": "*",
    "doctrine/collections": "^2.1",
    "ext-imap": "*"
  },
  "require-dev": {
    "tracy/tracy": "^2.8",
    "squizlabs/php_codesniffer": "3.*",
    "phpunit/phpunit": "^10.4",
    "dg/bypass-finals": "dev-master",
    "guzzlehttp/guzzle": "7.0"
  }
}
